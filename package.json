{
  "name": "minetrack",
  "version": "5.6.1",
  "description": "A Minecraft server tracker that lets you focus on the basics.",
  "dependencies": {
    "compression": "^1.7.4",
    "finalhandler": "^1.1.2",
    "mcpe-ping-fixed": "0.0.3",
    "mcping-js": "^1.4.1",
    "serve-static": "^1.16.2",
    "better-sqlite3": "^11.6.0",
    "apexcharts": "^3.45.1",
    "winston": "^3.11.0",
    "ws": "^7.5.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/memesalot/Minetrack.git"
  },
  "keywords": [
    "minetrack"
  ],
  "author": "Nick Krecklow <hello@npm.nklow.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/memesalot/Minetrack/issues"
  },
  "homepage": "https://github.com/memesalot/Minetrack#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "targets": {
    "main": {
      "engines": {
        "browsers": "defaults"
      },
      "includeNodeModules": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.23.0",
    "@babel/plugin-transform-class-properties": "^7.23.0",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "parcel": "^2.12.0"
  },
  "scripts": {
    "build": "eslint assets/js/*.js --rule 'eol-last: 0' && parcel build assets/html/index.html --dist-dir dist",
    "dev": "parcel build assets/html/index.html --no-optimize --dist-dir dist"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "mysql2": "^3.6.0",
    "utf-8-validate": "^5.0.2"
  }
}
